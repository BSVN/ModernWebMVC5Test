{
  "name": "test-vue",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "M6stafa <mostafa.najafi1996@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "npm run dev",
    "build": "node build/build.js",
    "e2e": "node test/e2e/runner.js",
    "jest": "tsc --noEmit --pretty -p . && jest --no-cache"
  },
  "dependencies": {
    "vue": "^2.5.1",
    "vue-resource": "^1.3.4",
    "vue-router": "^3.0.0",
    "vuex": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.2",
    "autoprefixer": "^7.1.5",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "chalk": "^2.1.0",
    "connect-history-api-fallback": "^1.4.0",
    "copy-webpack-plugin": "^4.1.1",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.7",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.4",
    "jest": "^21.2.1",
    "jest-serializer-vue": "^0.2.0",
    "jest-vue": "^0.8.1",
    "nightwatch": "^0.9.16",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.3.0",
    "portfinder": "^1.0.13",
    "rimraf": "^2.6.2",
    "semver": "^5.4.1",
    "shelljs": "^0.7.8",
    "ts-jest": "^21.1.3",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.3",
    "url-loader": "^0.6.2",
    "vue-class-component": "^6.0.0",
    "vue-loader": "^13.3.0",
    "vue-property-decorator": "^6.0.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.1",
    "vue-test-utils": "^1.0.0-beta.2",
    "webpack": "^3.7.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "ts",
      "vue"
    ],
    "transform": {
      "^.+\\.(js)$": "babel-jest",
      "^.+\\.(ts)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      ".*\\.(vue)$": "jest-vue"
    },
    "testMatch": [
      "**/test/jest/**/*.test.ts"
    ],
    "moduleNameMapper": {
      "vue$": "<rootDir>/node_modules/vue/dist/vue.js",
      "@": "<rootDir>/src"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "mapCoverage": true
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
